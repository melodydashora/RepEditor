{
  "name": "GPT Repository Access",
  "description": "Vecto Pilot AI (GPT-5) plans, diffs, commits and PRs. Tokens stay in the browser.",
  "icon": "icon.svg",
  "tools": [
    {
      "handler": "gpt-frame.html",
      "name": "AI Assistant",
      "icon": "icon.svg"
    },
    {
      "handler": "panel.html",
      "name": "Repo Fixer",
      "icon": "icon.svg"
    },
    {
      "handler": "sql.html",
      "name": "SQL Viewer",
      "icon": "icon.svg"
    },
    {
      "handler": "logs.html",
      "name": "Logs and Diagnostics",
      "icon": "icon.svg"
    }
  ],
  "fileHandlers": [
    {
      "glob": "*.svg",
      "handler": "svg-editor.html",
      "name": "SVG Editor",
      "icon": "icon.svg"
    },
    {
      "glob": "*.diff",
      "handler": "diff.html",
      "name": "Diff Viewer",
      "icon": "icon.svg"
    },
    {
      "glob": "*.patch",
      "handler": "diff.html",
      "name": "Patch Viewer",
      "icon": "icon.svg"
    }
  ]
}